{
  "name": "electron-local-transcode-test",
  "version": "1.0.3",
  "description": "Electron Local Transcode Test",
  "main": ".compiled/index.js",
  "scripts": {
    "compile": "./node_modules/.bin/babel src --out-dir .compiled --source-maps",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dist-win": "build --win --x64",
    "dist-mac": "build --mac"
  },
  "author": "",
  "license": "-",
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.0.13",
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "bluebird": "^3.5.1",
    "electron-window-state": "^4.1.1",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "electron": "^1.7.10",
    "electron-builder": "^19.52.1",
    "electron-rebuild": "^1.7.3"
  },
  "babel": {
    "sourceMaps": "inline",
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "build": {
    "appId": "jp.espresso3389.electron-local-transcode-test",
    "copyright": "(C) 2018 Takashi Kawasaki (@espresso3389). All rights reserved.",
    "asar": false,
    "productName": "Electron Local Transcode Test",
    "files": [
      ".compiled/*",
      ".compiled/**/*",
      "src/*",
      "src/**/*",
      "!.compiled/*.map",
      "!src/**/*.js"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico",
      "certificateSubjectName": "espresso3389"
    },
    "linux": {
      "icon": "build/icons"
    }
  }
}
